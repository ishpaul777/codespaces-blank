[
  {
    "id": "tagore:web:protected",
    "upstream": {
      "preserve_host": true,
      "url": "http://tagore-web:3000"
    },
    "match": {
      "url": "http://127.0.0.1:4455/.factly/tagore/web/<.*>",
      "methods": [
        "GET",
        "POST",
        "DELETE"
      ]
    },
    "authenticators": [
      {
        "handler": "cookie_session"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "id_token"
      }
    ],
    "errors": [
      {
        "handler": "redirect",
        "config": {
          "to": "http://127.0.0.1:4455/.factly/kavach/web/auth/login",
          "return_to_query_param": "return_to"
        }
      }
    ]
  }
]